{
    "prefix": {
        "markdown": "(markdown) => ",
        "code": "(code, language) => ",
        "blockquote": "(quote) => ",
        "html": "(html) => ",
        "heading": "(text, level) => ",
        "hr": "() => ",
        "list": "(body, ordered) => ",
        "listitem": "(text) => ",
        "paragraph": "(text) => ",
        "table": "(header, body) => ",
        "tablerow": "(content) => ",
        "tablecell": "(content, header, align) => ",
        "strong": "(text) => ",
        "em": "(text) => ",
        "codespan": "(code) => ",
        "br": "() => ",
        "del": "(text) => ",
        "link": "(href, title, text) => ",
        "image": "(href, title, text) => ",
        "text": "(text) => "
    },
    "suffix_default": {
        "markdown": "${(s=>{let e=document.createElement('textarea');e.innerHTML=s;return e.value})(markdown.split('[br]').join(' '))}",
        "code": "[code]${code}[/code]\\n",
        "blockquote": "[quote]${quote.split('[br]').join('\\n').replace(/\\n+$/,'')}[/quote]\\n",
        "html": "${((d,s)=>d.createElement('span').appendChild(d.createTextNode(s)).parentNode.innerHTML)(document,html)}",
        "heading": "[size=${[24,20,16,14,12.25,11.9][level-1]}px][b]${text}[/b][/size]\\n\\n",
        "hr": "[hr]\\n",
        "list": "[size=15px][list${ordered?'=1':''}]\\n${body.replace(/\\n+$/,'')}[/list][/size]\\n",
        "listitem": "[*]${text.replace(/\\n+$/,'')}\\n",
        "paragraph": "[size=15px]${text.split('\\n').join('[br]').split('[br][br]').join('\\n')}[/size]\\n\\n",
        "table": "[align=center][table=98%]${header}${body}[/table][/align]\\n",
        "tablerow": "[tr]${content}[/tr]\\n",
        "tablecell": "[td]${header?'[b]'+content+'[/b]':content}[/td]",
        "strong": "[b]${text}[/b]",
        "em": "[i]${text}[/i]",
        "codespan": "[size=13px][font=Monaco,Consolas,'Lucida Console','Courier New',serif][backcolor=#dedede]${code}[/backcolor][/font][/size]",
        "br": "[br][br]",
        "del": "[s]${text}[/s]",
        "link": "${text} [size=13px]([font=Monaco,Consolas,'Lucida Console','Courier New',serif]${href}[/font])[/size] ",
        "image": "[img]${href}[/img]",
        "text": "${text}"
    },
    "suffix_small": {
        "markdown": "${(s=>{let e=document.createElement('textarea');e.innerHTML=s;return e.value})(markdown.split('[br]').join(' '))}",
        "code": "[code]${code}[/code]\\n",
        "blockquote": "[quote]${quote.split('[br]').join('\\n').replace(/\\n+$/,'')}[/quote]\\n",
        "html": "${((d,s)=>d.createElement('span').appendChild(d.createTextNode(s)).parentNode.innerHTML)(document,html)}",
        "heading": "[size=${[21.6,19,14.4,12.6,12,11.8][level-1]}px][b]${text}[/b][/size]\\n\\n",
        "hr": "[hr]\\n",
        "list": "[size=12px][list${ordered?'=1':''}]\\n${body.replace(/\\n+$/,'')}[/list][/size]\\n",
        "listitem": "[*]${text.replace(/\\n+$/,'')}\\n",
        "paragraph": "[size=12px]${text.split('\\n').join('[br]').split('[br][br]').join('\\n')}[/size]\\n\\n",
        "table": "[align=center][table=98%]${header}${body}[/table][/align]\\n",
        "tablerow": "[tr]${content}[/tr]\\n",
        "tablecell": "[td]${header?'[b]'+content+'[/b]':content}[/td]",
        "strong": "[b]${text}[/b]",
        "em": "[i]${text}[/i]",
        "codespan": "[font=Monaco,Consolas,'Lucida Console','Courier New',serif][backcolor=#dedede]${code}[/backcolor][/font]",
        "br": "[br][br]",
        "del": "[s]${text}[/s]",
        "link": "${text} [size=13px]([font=Monaco,Consolas,'Lucida Console','Courier New',serif]${href}[/font])[/size] ",
        "image": "[img]${href}[/img]",
        "text": "${text}"
    }
}
